//Write a program that for an entered 
//character from the keyboard will check if it is 
//a vowel. 

#include <iostream>
using namespace std;

int main() {
    char letter;
    cin>>letter;

    if (letter == 'A' || letter == 'E' || letter == 'I' || letter == 'O' || letter == 'U' ||
        letter == 'a' || letter == 'e' || letter == 'i' || letter == 'o' || letter == 'u') {
        cout<<"Letter is a vowel"<<endl;
    }else {
        cout<<"Letter isn't vowel"<<endl;
    }
    return 0;
}
