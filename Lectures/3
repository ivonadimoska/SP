//Write a program that will calculate the sum 
//of all the numbers entered from the 
//keyboard as long as 0 is not entered.

#include <iostream>
using namespace std;

int main() {
    int number;
    int sum=0;

    do {
        cin>>number;
        sum+=number;
    }while (number!=0);

    cout<<"Sum = "<<sum;
    return 0;
}
