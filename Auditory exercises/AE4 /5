//Write a program that will determine the two numbers with
//the highest values from an unknown number of integers
//entered from the keyboard. The program ends if an invalid
//number representation is entered.
//Example:
//If the numbers 2 4 7 4 2 1 8 6 9 7 10 3 are entered, the program will print 10 and 9.

#include<iostream>
using namespace std;

int main() {
    int n, max1, max2, t;
    if (cin>>max1>>max2) {
        if (max2 > max1){
            t = max1;
            max1 = max2;
            max2 = t;
        }
        while(cin >> n) {
            if(n > max1){
                max2 = max1;
                max1 = n;
            } else if (n > max2) {
                max2 = n;
            }
        }
        cout << max1 << endl;
        cout << max2 << endl;
    } else {
        cout << "Vnesi minimum dva broja";
    }
    return 0;
}
}
