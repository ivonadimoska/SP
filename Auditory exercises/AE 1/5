//Write a program that prints the remainders when the number 19 is divided
//by 2, 3, 5, and 8.

#include <iostream>
using namespace std;

int main() {
    int num = 19;

    cout<<"Remainder of 19/2 is: "<<num%2<<endl;
    cout<<"Remainder of 19/3 is: "<<num%3<<endl;
    cout<<"Remainder of 19/5 is: "<<num%5<<endl;
    cout<<"Remainder of 19/8 is: "<<num%8<<endl;

    return 0;
}
