//Write a program that reads two integers from the standard input
//and prints their sum, difference, product, and remainder when divided.

#include <iostream>
using namespace std;

int main() {
    int a,b;
    cin>>a>>b;

    cout<<"Sum = "<<a+b<<endl;
    cout<<"Difference = "<<a-b<<endl;
    cout<<"Product = "<<a*b<<endl;
    cout<<"Remainder = "<<a%b<<endl;

    return 0;
}
